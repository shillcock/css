<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<plugin-list>
   <plugin name="dmzWebServicesModuleQt" scope="web-services"/>
   <plugin name="dmzObjectPluginWebServices" scope="web-services-object"/>
   <plugin name="dmzWebServicesPluginAutoTrackUndo"/>
</plugin-list>
<web-services>
  <server db="stance">
  <!-- <server host="dmz.couchone.com" db="test"> -->
  <!-- <server host="games.chds.us" port="443" path="/db/" db="test"> -->
<!--     <proxy host="localhost" port="8888"/>-->
  </server>
</web-services>
<web-services-object>
   <filter>
      <object-type-set>
        <object-type name="Tool"/>
        <object-type name="Player"/>
      </object-type-set>
      <attribute name="Object_Select_Attribute">
         <mask name="flag"/>
      </attribute>
      <state name="Selected"/>
      <state name="Highlight"/>
   </filter>
</web-services-object>
<runtime>
   <object-type name="Player"/>
 </runtime>
<archive>
   <object name="hil" type="Player">
      <attributes name="Object_Human_In_The_Loop">
         <flag value="true"/>
      </attributes>
   </object>
</archive>
</dmz>
